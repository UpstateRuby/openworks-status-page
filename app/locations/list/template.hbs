<div class="row">
  <div class="col-md-12">
    <h2>Locations</h2>
  </div>
</div>
<div class="row">
  {{#each model as |location|}}
      <div class="col-md-3">
        {{#link-to "locations.show" location}}{{location.name}}{{/link-to}}
      </div>
  {{else}}
      <div class="col-md-12">
        {{empty-state "No Locations Yet"}}
      </div>
  {{/each}}
</div>
{{outlet}}
